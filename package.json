{
  "name": "turbo-template",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "catalog:lint",
    "turbo": "catalog:repo",
    "typescript": "catalog:dev"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18"
  }
}